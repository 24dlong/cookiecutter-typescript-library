{
  "type": "module",
  "engines": {
    "node": ">=22.0.0 <23",
    "pnpm": ">=10.0.0 <11"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.33.0",
    "@semantic-release/changelog": "6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "semantic-release": "24.2.7",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "vitest": "3.2.4"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint:actionlint": "./.cache/bin/actionlint -color ./.github/workflows/*.yml",
    "lint": "eslint . && prettier . --check --ignore-unknown && pnpm lint:actionlint",
    "lint:fix": "eslint --fix && prettier . --write --ignore-unknown && pnpm lint:actionlint",
    "setup:actionlint": "./scripts/setup-actionlint.sh",
    "prepare": "husky"
  }
}
